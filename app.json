{
  "expo": {
    "name": "ProjetoQuinze",
    "slug": "ProjetoQuinze",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.andre0luis.clubequinze",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "clubequinze",
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "Precisamos acessar sua galeria para anexar imagens aos posts e perfil."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "65023eb1-bfbe-480c-9781-4441576253d9"
      }
    }
  }
}
